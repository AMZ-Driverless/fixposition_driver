<launch>
    <!--Change input_type and input_port according to the type of input source:
               tcp - data over TCP
               serial - data over a serial port 
        And according to the port used for reading, i.e.
               21000 - TCP port
               /dev/ttyUSB0 - serial port -->
    <arg name="input_type" default="tcp"/>
    <arg name="input_port" default="21000"/>

    <arg name="input_format" default="fp"/>
    <arg name="tcp_ip" default="192.168.49.1"/>
    <arg name="serial_baudrate" default="115200"/>
    <arg name="pub_rate" default="200"/>


    <node name="fp_output" pkg="fixposition_output" type="fixposition_output" output="screen" respawn="true" respawn_delay="5">
        <param name="input_type" type="string" value="$(arg input_type)" />
        <param name="input_port" type="string" value="$(arg input_port)" />
        <param name="input_format" type="string" value="$(arg input_format)" />
        <param name="tcp_ip" type="string" value="$(arg tcp_ip)" />
        <param name="serial_baudrate" type="int" value="$(arg serial_baudrate)" />
        <param name="pub_rate" type="int" value="$(arg pub_rate)" />


    </node>
</launch>

